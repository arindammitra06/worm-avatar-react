{
  "version": "1.0.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "worm-avatar-react",
  "author": "Arindam Mitra",
  "homepage": "https://github.com/arindammitra06/worm-avatar-react",
  "license": "Apache License 2",
  "repository": "github:arindammitra06/worm-avatar-react",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "keywords": [
    "react-component",
    "avatar",
    "worm-avatar",
    "cute-avatars",
    "funny-avatars",
    "react-avatars"
  ],
  "module": "dist/worm-avatar-react.esm.js",
  "size-limit": [
    {
      "path": "dist/worm-avatar-react.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/worm-avatar-react.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.2.6",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "husky": "^8.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "size-limit": "^8.2.6",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  }
}
